import type { Store, Reducer } from 'redux';

declare module 'redux-commander' {
  declare export type Command =
    | []
    | [() => any]
    | [(any) => any, any]
    | [(any, any) => any, any, any]
    | [(any, any, any) => any, any, any, any]
    | [(any, any, any, any) => any, any, any, any, any]
    | [(any, any, any, any, any) => any, any, any, any, any, any]
    | [(any, any, any, any, any, any) => any, any, any, any, any, any, any]
    | [
        (any, any, any, any, any, any, any) => any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
      ]
    | [
        (any, any, any, any, any, any, any, any) => any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
      ]
    | [
        (any, any, any, any, any, any, any, any, any) => any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
      ]
    | [
        (any, any, any, any, any, any, any, any, any, any) => any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
        any,
      ];

  declare export type CommandReducer<S, A> = (
    state: S | void,
    action: A
  ) =>
    | S
    | [S, Command]
    | [S, Command, Command]
    | [S, Command, Command, Command]
    | [S, Command, Command, Command, Command]
    | [S, Command, Command, Command, Command, Command]
    | [S, Command, Command, Command, Command, Command, Command]
    | [S, Command, Command, Command, Command, Command, Command, Command]
    | [
        S,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
      ]
    | [
        S,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
        Command,
      ];

  declare export function reduceCommandReducers<S, A>(
    store: Store<S, A>,
    Array<CommandReducer<S, A>>
  ): Reducer<S, A>;

  declare export function combineCommandReducers<S, A>(
    store: Store<S, A>,
    { [key: string]: CommandReducer<S, A>[] }
  ): Reducer<S, A>;
}
