    "babel-preset-env": "^1.6.1",
    "flow-bin": "^0.66.0",
    "jest": "^22.4.2",
    "redux": "^4.0.0",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2"
  },
  "author": "",
